@model StreamDream.Models.Customer
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h4>Details</h4>
    <h2>@Model.Name</h2>
    <ul>
        <li>Membership Type: @Model.MembershipType.Name</li>
        <li>BirthDate: @Model.Birthdate</li>
    </ul>

    <a href="@Url.Action("Edit",new {@Model.Id })">Edit Customer</a>
